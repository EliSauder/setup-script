#!/bin/bash


find ../user_scripts/ \( -name "*_logout_pre" \
    -and -not -name "*zsh*" -and -not "*bash*" \
    -and -not -name "*csh*" -and -not "*fish*" \) | xargs -0 source

find ../user_scripts/ \( -name "*_logout" \
    -and -not -name "*zsh*" -and -not "*bash*" \
    -and -not -name "*csh*" -and -not "*fish*" \) | xargs -0 source

find ../user_scripts/ \( -name "*_logout_post" \
    -and -not -name "*zsh*" -and -not "*bash*" \
    -and -not -name "*csh*" -and -not "*fish*" \) | xargs -0 source
