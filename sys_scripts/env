#!/bin/bash

find ../user_scripts/ \( -name "*_env_pre" \
    -and -not -name "*zsh*" -and -not "*bash*" \
    -and -not -name "*csh*" -and -not "*fish*" \) | xargs -0 source

find ../user_scripts/ \( -name "*_env" \
    -and -not -name "*zsh*" -and -not "*bash*" \
    -and -not -name "*csh*" -and -not "*fish*" \) | xargs -0 source

find ../user_scripts/ \( -name "*_env_post" \
    -and -not -name "*zsh*" -and -not "*bash*" \
    -and -not -name "*csh*" -and -not "*fish*" \) | xargs -0 source
