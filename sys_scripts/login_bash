#!/bin/bash

source "$SCRIPT_DIRECTORY/sys_scripts/login"

find "$SCRIPT_DIRECTORY/user_scripts/" -name "*bash_login_pre" -exec source {} \;

find "$SCRIPT_DIRECTORY/user_scripts/" -name "*bash_login" -exec source {} \;

find "$SCRIPT_DIRECTORY/user_scripts/" -name "*bash_login_post" -exec source {} \;
