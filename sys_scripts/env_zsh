#!/bin/bash

source "$SCRIPT_DIRECTORY/sys_scripts/env"

while read file; do
    source "$file"
done <<< "$(find "$SCRIPT_DIRECTORY/user_scripts/" -name "*zsh_env_pre"  )"

while read file; do
    source "$file"
done <<< "$(find "$SCRIPT_DIRECTORY/user_scripts/" -name "*zsh_env"  )"

while read file; do
    source "$file"
done <<< "$(find "$SCRIPT_DIRECTORY/user_scripts/" -name "*zsh_env_post"  )"
